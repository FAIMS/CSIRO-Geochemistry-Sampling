/* Loads the most recetly clicked on existing sample for editing. */
loadSample() {
    String sampleID = getListItemValue();
    if (!isNull(sampleID)) {
        fetchAll("select aenttypename from latestnondeletedarchent join aenttype using (aenttypeid) where uuid = '" + sampleID + "';", 
         new FetchCallback() {
            onFetch(result) {
                // Perform any sample initialisation 
                currentSampleUUID = sampleID;
                photolog_id = null;

                print("[loadSample()] Loading a sample, currentSampleUUID = " + currentSampleUUID);
                if (result.get(0).get(0).equals("Water Sample")) {
                    setFieldValue("Project/Start/Sample_Types", "Water Sample");
                    loadWaterSampleFrom(sampleID);
                } else if (result.get(0).get(0).equals("Plant Sample")) {
                    setFieldValue("Project/Start/Sample_Types", "Plant Sample");
                    loadPlantSampleFrom(sampleID);
                } else if (result.get(0).get(0).equals("Soil Sample")) {
                    setFieldValue("Project/Start/Sample_Types", "Soil Sample");
                    loadSoilSampleFrom(sampleID);
                } else {
                    setFieldValue("Project/Start/Sample_Types", "Rock Sample");
                    loadRockSampleFrom(sampleID);
                }
            }
        });
    }    
}

/* Returns a name value pair for the given attribute. */
copyAttribute(String attrib) {
    value = getFieldValue(attrib);
    pair = new ArrayList();
    pair.add(attrib);
    pair.add(value);
    return pair;   
}

/* Adds the field value of a view as an attribute to the given attribute list. 
    Not for use with multiselection views.  
   propertyName is the property name in the data_schema.
   viewRef is the path to the field to duplicate.
   isMeasure differentiates between vocab and measure views.
   */
addAttribute(attributeList, String propertyName, String viewRef, boolean isMeasure) {
    if (!isNull(getFieldValue(viewRef))) {
        print("[addAttribute()]" + getFieldValue(viewRef));
        if (isMeasure) { 
            attributeList.add(createEntityAttribute(propertyName, getFieldAnnotation(viewRef), null, getFieldValue(viewRef), getFieldCertainty(viewRef)));
        } else {
            attributeList.add(createEntityAttribute(propertyName, getFieldAnnotation(viewRef), getFieldValue(viewRef), null, getFieldCertainty(viewRef)));
        }
    }
}

/* Adds the field value of a view as an attribute to the given attribute list. 
    Only for use with multi-selection checkboxes and radiobuttons. 
   propertyName is the property name in the data_schema.
   viewRef is the path to the field to duplicate.
   */
addMultSelectionAttribute(attributeList, String propertyName, String viewRef) {
    if (!isNull(getFieldValue(viewRef))) {
        print("[addCheckboxAttribute()]" + getFieldValue(viewRef)); 
        for (listItem : getFieldValue(viewRef)) {
            print(listItem.getName());
            attributeList.add(createEntityAttribute(propertyName, null, listItem.getName(), null, null));
        }       
    }
}

