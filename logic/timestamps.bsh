/********************************* TIMESTAMPS *********************************/

// Overrides autogenerated function
String getTimestampNow() {
  String fmt = "ddMMMyy";
  return getTimestampNow(fmt);
}

// Overrides autogenerated function
String getTimestampNow(String fmt) {
  date    = new Date();
  dateFmt = new java.text.SimpleDateFormat(fmt);
  dateStr = dateFmt.format(date);

  return dateStr;
}
