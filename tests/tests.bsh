assert(digLen("0000", 4));
assert(digLen("0001", 4));
assert(digLen("1000", 4));
assert(digLen("1", 1));
assert(digLen("", 0));

assert( isInRange("0",    "0",        "0"));
assert(!isInRange("0",    "1",       "-1"));
assert(!isInRange("1",    "0",        "0"));
assert(!isInRange("0002", "0000",  "0001"));
assert( isInRange("0042", "0000",  "0100"));

